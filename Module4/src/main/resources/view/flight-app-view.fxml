<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane minWidth="800.0" prefHeight="290.0" prefWidth="451.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.au.cpsc.module4.FlightDesignator.controller.FlightScheduleAppController">
   <center>
      <fx:include fx:id="flightTableView" maxWidth="Infinity" source="flight-table-view.fxml" />
   </center>
   <bottom>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <fx:include fx:id="flightDetailView" maxWidth="Infinity" source="flight-detail-view.fxml" />
            <HBox prefHeight="34.0" prefWidth="451.0">
               <children>
                  <Button fx:id="updateButton" minWidth="50.0" mnemonicParsing="false" onAction="#updateButtonAction" text="Update" />
                  <Button fx:id="newButton" minWidth="50.0" mnemonicParsing="false" onAction="#newButtonAction" text="New">
                     <HBox.margin>
                        <Insets left="3.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="deleteButton" minWidth="50.0" mnemonicParsing="false" onAction="#deleteButtonAction" text="Delete">
                     <HBox.margin>
                        <Insets left="3.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" top="5.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </bottom>
</BorderPane>
